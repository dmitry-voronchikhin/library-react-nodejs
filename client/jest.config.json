{
  "collectCoverageFrom": [
    "src /**/*.{js,jsx}"
  ],
  "coverageDirectory": "coverage",
  "testEnvironment": "jest-environment-jsdom",
  "extensionsToTreatAsEsm": [
    ".tsx"
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/jest.setup.js"
  ],
  "transformIgnorePatterns": [
    "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
  ]
}